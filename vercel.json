{
  "rewrites": [
    { "source": "/favicon.ico", "destination": "/images/logo.png" },
    { "source": "/images/favicon.png", "destination": "/images/logo.png" }
  ]
}
{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
